{
  "name": "seothemes/compat-checker",
  "description": "Composer package to check PHP, WordPress and Genesis compatibility.",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "seothemes",
      "email": "seothemeswp@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.2.4"
  },
  "autoload": {
    "files": [
      "src/class-compat-checker.php"
    ]
  },
  "extra": {
    "compat-checker": {
      "plugin_slug": "compat-checker/example-plugin.php",
      "plugin_name": "Compat Checker",
      "min_php_version": "5.4.0",
      "min_wp_version": "5.0.0",
      "min_genesis_version": "2.8.0",
      "require_genesis": "true",
      "require_child_theme": "true"
    }
  }
}
